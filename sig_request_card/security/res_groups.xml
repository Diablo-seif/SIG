<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="category_request_card" model="ir.module.category">
        <field name="name">Request Card</field>
    </record>

    <record id="group_access_request_card" model="res.groups">
        <field name="name">Access Request Card</field>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        <field name="category_id" ref="category_request_card"/>
    </record>

    <record id="group_create_request_card" model="res.groups">
        <field name="name">Create Request Card</field>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        <field name="category_id" ref="category_request_card"/>
    </record>
</odoo>