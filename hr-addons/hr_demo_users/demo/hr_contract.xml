        <field name="name">Contract For Test Employee 1</field>
        <field name="notes">This is Test Employee 1's contract</field>
        <field name="employee_id" ref="employee_employee"/>
        <field name="department_id" model="hr.department"
               eval="obj().env.ref('hr_demo_users.employee_employee').department_id.id"/>
        <field name="date_start" eval="(datetime.now()).strftime('%Y-01-01')"/>
        <field eval="5000.0" name="wage"/>
        <field name="state">open</field>
        <field name="kanban_state">normal</field>
