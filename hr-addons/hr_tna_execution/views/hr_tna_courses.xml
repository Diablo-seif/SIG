<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="execution_hr_tna_courses_kanban" model="ir.ui.view">
        <field name="name">execution.hr.tna.courses.kanban</field>
        <field name="model">hr.tna.courses</field>
        <field name="inherit_id" ref="hr_tna.hr_tna_courses_kanban"/>
        <field name="arch" type="xml">
            <xpath expr="//div[hasclass('oe_kanban_details')]/ul" position="inside">
                <t t-if="record.date_from.raw_value"><li>Start : <field name="date_from"/></li></t>
                <t t-if="record.date_to.raw_value"><li>End : <field name="date_to"/></li></t>
            </xpath>
        </field>
    </record>
</odoo>
