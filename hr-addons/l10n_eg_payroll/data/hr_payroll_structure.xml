<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

        <record id="full_time_salary_structure" model="hr.payroll.structure">
            <field name="name">Full Time</field>
            <field name="type_id" ref="hr_contract.structure_type_employee"/>
            <field name="unpaid_work_entry_type_ids" eval="[(4, ref('hr_work_entry_contract.work_entry_type_unpaid_leave'))]"/>
            <field name="country_id" eval="False"/>
        </record>

        <record id="owner_salary_structure" model="hr.payroll.structure">
            <field name="name">Owner</field>
            <field name="type_id" ref="hr_contract.structure_type_employee"/>
            <field name="unpaid_work_entry_type_ids" eval="[[(4, ref('hr_work_entry_contract.work_entry_type_unpaid_leave'))]]"/>
            <field name="country_id" eval="False"/>
        </record>

</odoo>
