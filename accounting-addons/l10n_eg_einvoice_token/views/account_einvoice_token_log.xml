<odoo>
    <record id="account_einvoice_token_log_form" model="ir.ui.view">
        <field name="name">account.einvoice.token.log.form</field>
        <field name="model">account.einvoice.token.log</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="request_url"/>
                            <field name="request_method"/>
                            <field name="request_header"/>
                            <field name="request_data"/>
                        </group>
                        <group>
                            <field name="request_params"/>
                            <field name="result"/>
                        </group>
                    </group>
                    <group>
                        <group>
                            <field name="create_date"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="account_einvoice_token_log_tree" model="ir.ui.view">
        <field name="name">account.einvoice.token.log.tree</field>
        <field name="model">account.einvoice.token.log</field>
        <field name="arch" type="xml">
            <list>
                <field name="id"/>
                <field name="request_method"/>
                <field name="create_date"/>
                <field name="request_url" optional="hide"/>
                <field name="request_header" optional="hide"/>
                <field name="request_data" optional="hide"/>
                <field name="request_params" optional="hide"/>
                <field name="result" optional="hide"/>
            </list>
        </field>
    </record>

    <record id="account_einvoice_token_log_action"
            model="ir.actions.act_window">
        <field name="name">E-Invoice Token Log</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">account.einvoice.token.log</field>
        <field name="view_mode">list,form</field>
    </record>

    <record id="account_einvoice_token_log_menu" model="ir.ui.menu">
        <field name="name">E-Invoice Token Log</field>
        <field name="action" ref="account_einvoice_token_log_action"/>
        <field name="parent_id" ref="l10n_eg_einvoice.einvoice_category_menu"/>
    </record>

</odoo>
