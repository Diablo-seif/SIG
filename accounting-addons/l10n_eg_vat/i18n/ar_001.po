"                    Dear ${object.name}\n"
"                    <br/><br/>\n"
"                    Please check VAT report for month\n"
"                    % if datetime.date.today().month == 1:\n"
"                        11, ${datetime.date.today().year-1}\n"
"                    % else:\n"
"                        % if datetime.date.today().month == 2:\n"
"                            12, ${datetime.date.today().year-1}\n"
"                        % else:\n"
"                            ${datetime.date.today().month-2},\n"
"                            ${datetime.date.today().year}\n"
"                        % endif\n"
"                    % endif\n"
"                    <br/><br/>\n"
"                </p>\n"
"            </div>\n"
"        "
