<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="create_rfq_form" model="ir.ui.view">
        <field name="name">create.rfq.form</field>
        <field name="model">create.rfq</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="proposal_id" invisible="1"/>
                    <field name="partner_ids" widget="many2many_tags"/>
                </group>
                <footer>
                    <button name="action_create_rfq" string="Create Rfq" type="object" class="oe_highlight"/>
                    <button string="Cancel" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="create_rfq_action" model="ir.actions.act_window">
        <field name="name">Create Rfq</field>
        <field name="res_model">create.rfq</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>

</odoo>
